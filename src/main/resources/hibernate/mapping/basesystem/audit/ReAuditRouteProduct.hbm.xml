<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cs.lexiao.admin.mapping.basesystem.audit.ReAuditRouteProduct" table="RE_AR_PROD">
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="22" scale="0" />
            <generator class="native"></generator>
        </id>
        <!--version标签必须跟在id标签后面-->
    	<version name="version" column="VER" type="java.lang.Long"/>
        <property name="auditRouteId" type="java.lang.Long">
            <column name="AR_ID" />
        </property>
        <property name="prodId" type="java.lang.Long">
            <column name="PROD_ID" />
        </property>
        <property name="brchId" type="java.lang.Long">
            <column name="BRCH_ID" />
        </property>
    </class>
</hibernate-mapping>
