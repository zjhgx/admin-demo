<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cs.lexiao.admin.mapping.basesystem.product.MemberProductAttribute" table="MEMBER_PRODUCT_ATTRIBUTE">
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="22" scale="0" />
            <generator class="native" />
        </id>
        <property name="miNo" type="java.lang.String">
            <column name="MI_NO" length="50" />
        </property>
        <property name="productId" type="java.lang.Long">
            <column name="PRODUCT_ID" precision="22" scale="0" />
        </property>
        <property name="attributeId" type="java.lang.Long">
            <column name="ATTRIBUTE_ID" precision="22" scale="0" />
        </property>
        <property name="attributeKey" type="java.lang.String">
            <column name="ATTRIBUTE_KEY" length="100" />
        </property>
        <property name="attributeValue" type="java.lang.String">
            <column name="ATTRIBUTE_VALUE" length="100" />
        </property>
    </class>
</hibernate-mapping>
