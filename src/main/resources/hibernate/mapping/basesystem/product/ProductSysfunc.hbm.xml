<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cs.lexiao.admin.mapping.basesystem.product">
	<class name="ProductSysfunc" table="PRODUCT_SYSFUNC">
		<id name="id" type="java.lang.Long">
			<column name="ID" sql-type="NUMBER" length="22" />
			<generator class="native" />
		</id>
		<property name="productId" type="java.lang.Long">
			<column name="PRODUCT_ID" length="22"/>
		</property>
		<property name="funcId" type="java.lang.Long">
			<column name="FUNC_ID" length="22"/>
		</property>
		<property name="status" type="java.lang.String">
			<column name="STATUS" length="1" />
		</property>
	</class>
</hibernate-mapping>