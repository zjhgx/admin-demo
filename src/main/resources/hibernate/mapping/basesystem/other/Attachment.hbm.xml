<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cs.lexiao.admin.mapping.basesystem.other">
	<class name="Attachment" table="SYS_ATTACHMENT">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="creator" column="CREATOR" />
		<property name="createTime" column="CREATE_TIME" />
		<property name="name" column="NAME" />
		<property name="saveName" column="SAVE_NAME" />
		<property name="attachPath" column="ATTACH_PATH" />
		<property name="attachSize" column="ATTACH_SIZE" />
		<property name="uploadType" column="UPLOAD_TYPE" />
		<property name="domain" column="domain" />
		<property name="xhhResult" column="xhh_result" />
	</class>
</hibernate-mapping>